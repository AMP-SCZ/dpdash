// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DPDATA_DATABASE_URL")
}

type charts {
        id       Int      @map("_id")
title String
variable String
assessment String
description String
fieldLabelValueMap labelValue[]
public Boolean
owner String
updatedAt DateTime

}

type labelValue {
  value String
  label String
  color String
  targetValues any
}
